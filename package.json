{
  "name": "juju-dashboard",
  "version": "0.13.3",
  "description": "A dashboard for Juju and JAAS (Juju as a service)",
  "bugs": {
    "url": "https://github.com/canonical/juju-dashboard/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canonical/juju-dashboard.git"
  },
  "type": "module",
  "license": "LGPL-3.0",
  "author": {
    "name": "Canonical Webteam",
    "email": "webteam@canonical.com"
  },
  "main": "index.js",
  "scripts": {
    "build": "vite build && yarn run generate-version-file",
    "clean": "./scripts/clean-files",
    "eslint-check": "eslint src/",
    "generate-release-tarball": "yarn run build && ./scripts/generate-release-tarball",
    "generate-version-file": "./scripts/generate-version-file",
    "lint": "npmPkgJsonLint . && yarn run eslint-check && yarn run prettier-check && yarn run stylelint-check",
    "prettier-check": "prettier --check 'src/**/*'",
    "serve": "yarn run start",
    "start": "vite -c vite.dev.config.ts",
    "start-jaas": "vite -c vite.jaas.config.ts",
    "stylelint-check": "stylelint 'src/**/*.scss'",
    "stylelint-fix": "stylelint --fix 'src/**/*.scss'",
    "test": "vitest",
    "typescript-build": "tsc -b"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@canonical/jujulib": "7.0.0",
    "@canonical/macaroon-bakery": "1.3.2",
    "@canonical/react-components": "1.7.3",
    "@canonical/rebac-admin": "0.0.1-alpha.8",
    "@reduxjs/toolkit": "2.5.0",
    "@sentry/browser": "8.47.0",
    "@sentry/core": "8.47.0",
    "ansi-to-html": "0.7.2",
    "async-limiter": "2.0.0",
    "classnames": "2.5.1",
    "clone-deep": "4.0.1",
    "d3": "7.9.0",
    "date-fns": "4.1.0",
    "dompurify": "3.2.3",
    "formik": "2.4.6",
    "framer-motion": "11.16.0",
    "fuse.js": "7.0.0",
    "lodash.isequal": "4.5.0",
    "lodash.mergewith": "4.6.2",
    "prism-react-renderer": "2.4.1",
    "prismjs": "1.29.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-ga": "3.3.1",
    "react-hot-toast": "2.5.1",
    "react-json-tree": "0.19.0",
    "react-redux": "9.2.0",
    "react-router": "7.1.1",
    "react-useportal": "1.0.19",
    "redux": "5.0.1",
    "vanilla-framework": "4.18.5",
    "yup": "1.6.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/classnames": "2.3.4",
    "@types/clone-deep": "4.0.4",
    "@types/d3": "7.4.3",
    "@types/dompurify": "3.2.0",
    "@types/lodash.isequal": "4.5.8",
    "@types/lodash.mergewith": "4.6.9",
    "@types/node": "18.19.70",
    "@types/prismjs": "1.26.5",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/react-table": "7.7.20",
    "@types/redux-mock-store": "1.5.0",
    "@typescript-eslint/eslint-plugin": "8.19.1",
    "@typescript-eslint/parser": "8.19.1",
    "@vitejs/plugin-basic-ssl": "1.2.0",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/coverage-v8": "2.1.8",
    "axios": "1.7.9",
    "axios-mock-adapter": "2.1.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.3",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.16",
    "eslint-plugin-vitest": "0.5.4",
    "fishery": "2.2.2",
    "happy-dom": "16.3.0",
    "jest-websocket-mock": "2.5.0",
    "npm-package-json-lint": "8.0.0",
    "postcss": "8.4.49",
    "prettier": "3.4.2",
    "react-anchorme": "4.0.1",
    "redux-devtools": "3.7.0",
    "redux-devtools-extension": "2.13.9",
    "redux-mock-store": "1.5.5",
    "sass": "1.83.1",
    "stylelint": "16.12.0",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-order": "6.0.4",
    "typescript": "5.7.2",
    "vite": "6.0.7",
    "vite-plugin-html": "3.2.2",
    "vite-plugin-node-polyfills": "0.22.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.8",
    "vitest-fetch-mock": "0.4.3"
  },
  "npmpackagejsonlint": {
    "rules": {
      "prefer-absolute-version-dependencies": "error",
      "prefer-absolute-version-devDependencies": "error",
      "prefer-alphabetical-dependencies": "error",
      "prefer-alphabetical-devDependencies": "error"
    }
  },
  "packageManager": "yarn@4.6.0",
  "resolutions": {
    "anchorme": "^3.0.8"
  }
}
